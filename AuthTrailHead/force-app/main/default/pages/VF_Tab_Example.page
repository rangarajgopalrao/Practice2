<apex:page controller="Tab_Example">
    <apex:form id="fm">
        <apex:tabPanel value="{!selected}" switchType="server" width="800" height="200">
            <apex:tab label="Basic Information" name="basic" labelWidth="200">
                <apex:pageBlock mode="MainDetails">
                    <apex:pageBlockButtons location="bottom" dir="rtl">
                        <apex:commandButton value="next" action="{!next}"/>
                   </apex:pageBlockButtons>
                    <apex:pageBlockSection columns="1">
                        <apex:pageBlockSectionItem >
                            <apex:outputLabel value="Last Name"/>
                            <apex:inputText />
                       </apex:pageBlockSectionItem>
                        <apex:pageBlockSectionItem >
                            <apex:outputLabel value="First Name"/>
                            <apex:inputText />
                       </apex:pageBlockSectionItem>
                        <apex:pageBlockSectionItem >
                            <apex:outputLabel value="Age"/>
                            <apex:inputText />
                       </apex:pageBlockSectionItem>
                   </apex:pageBlockSection>
               </apex:pageBlock>
           </apex:tab>
            <apex:tab label="Communication" name="comp" labelWidth="200">
                <apex:pageBlock mode="MainDetails">
                    <apex:pageBlockButtons location="bottom" dir="rtl">
                        <apex:commandButton value="Previous" action="{!previous}"/>
                        <apex:commandButton value="finish" action="{!finish}"/>
                   </apex:pageBlockButtons>
                    <apex:pageBlockSection columns="1">
                        <apex:pageBlockSectionItem >
                            <apex:outputLabel value="City"/>
                            <apex:inputText />
                       </apex:pageBlockSectionItem>
                        <apex:pageBlockSectionItem >
                            <apex:outputLabel value="Place"/>
                            <apex:inputText />
                       </apex:pageBlockSectionItem>
                        <apex:pageBlockSectionItem >
                            <apex:outputLabel value="Country"/>
                            <apex:inputText />
                       </apex:pageBlockSectionItem>
                   </apex:pageBlockSection>
               </apex:pageBlock>
           </apex:tab>
       </apex:tabPanel>
   </apex:form>
</apex:page>