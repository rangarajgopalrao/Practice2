<apex:page controller="PRACT_VALID_TRY_CATCH" sidebar="false">
    <apex:pageMessages />
    <apex:form >
        <apex:pageBlock >
            <apex:pageBlockSection >
                
                <apex:pageBlockSectionItem >
                    <apex:outputLabel value="Name"/>
                    <apex:inputText value="{!name}"/>
               </apex:pageBlockSectionItem>
                
                <apex:pageBlockSectionItem >
                    <apex:outputLabel value="anum"/>
                    <apex:inputText value="{!anum}"/>
               </apex:pageBlockSectionItem>
                
           </apex:pageBlockSection>
            
            <apex:pageBlockButtons >
                <apex:commandButton value="submit" action="{!submit}" reRender=" "/>
           </apex:pageBlockButtons>
       </apex:pageBlock>
   </apex:form>
</apex:page>