<apex:page controller="PRACTICE_PR_SOSL_02">
    <apex:form >
        <apex:pageBlock >
           <apex:pageBlockSection >
               <apex:pageBlockSectionItem >
                   <apex:outputLabel >ENTER THE SEARCH TEXT</apex:outputLabel>
                   <apex:inputText value="{!searchtext}" />    
              </apex:pageBlockSectionItem>
               <apex:commandButton value="search" action="{!dosearch}"/>
          </apex:pageBlockSection>
            
            <apex:pageBlockSection columns="1">
                <apex:pageBlockTable value="{!output}" var="O">
                    <apex:column value="{!O.name}"/>
               </apex:pageBlockTable>
           </apex:pageBlockSection>
       </apex:pageBlock>
   </apex:form>
</apex:page>